            </div>
            <div class="form-group">
                <label asp-for="@Model.DevT.DevTname" class="control-label"></label>
                <input asp-for="@Model.DevT.DevTname" class="form-control" />
                @*  <span asp-validation-for="@Model.DevT.LastName" class="text-danger"></span> *@
            </div>
           

            <div class="form-group">
                <input type="submit" value="Save" class="btn btn-primary" />
