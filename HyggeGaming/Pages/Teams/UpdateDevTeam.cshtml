}

<h1>Update</h1>

<h4>Team</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form method="post">
            <input type="hidden" asp-for="DevT" />
            <div asp-validation-summary="ModelOnly" class="text-danger"></div> 
            <div class="form-group">
